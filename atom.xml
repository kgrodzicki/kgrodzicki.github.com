---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">

    <title>kgrodzicki.com</title>
    <link href="http://kgrodzicki.com/"/>
    <link type="application/atom+xml" rel="self" href="http://kgrodzicki.com/atom.xml"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://kgrodzicki.com/</id>
    <author>
        <name>Krzysztof Grodzicki</name>
        <email>kgrodzicki@gmail.com</email>
    </author>

    {% for post in site.posts %}
    <entry>
        <id>http://kgrodzicki.com{{ post.id }}</id>
        <link type="text/html" rel="alternate" href="http://kgrodzicki.com{{ post.url }}"/>
        <title>{{ post.title }}</title>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <author>
            <name>Krzysztof Grodzicki</name>
            <uri>http://kgrodzicki.com/</uri>
        </author>
        <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
    {% endfor %}

</feed>
